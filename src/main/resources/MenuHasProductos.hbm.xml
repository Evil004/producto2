<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.example.producto2.models.entity.MenuHasProductos" table="menu_has_productos" schema="paracasa">
        <composite-id mapped="true" class="org.example.producto2.models.entity.MenuHasProductosPK">
            <key-property name="idMenu" column="id_menu"/>
            <key-property name="idProducto" column="id_producto"/>
        </composite-id>
    </class>
</hibernate-mapping>