<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.example.producto2.models.entity.UsuarioHasMenus" table="usuario_has_menus" schema="paracasa">
        <composite-id mapped="true" class="org.example.producto2.models.entity.UsuarioHasMenusPK">
            <key-property name="idMenu" column="id_menu"/>
            <key-property name="idUsuario" column="id_usuario"/>
        </composite-id>
    </class>
</hibernate-mapping>